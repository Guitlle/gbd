	keywords = {Antiretroviral Therapy, Highly Active, Botswana, Communicable Disease Control, Computer Simulation, {HIV} Infections, {HIV} Seroprevalence, India, Infectious Disease Transmission, Vertical, Pregnancy, Pregnancy Complications, Infectious, Program Evaluation, Prostitution},
