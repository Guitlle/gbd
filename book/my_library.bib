	pages = {2468--2481}
},

@article{gutierrez-delgado_[generalized_2008,
	title = {{[Generalized} cost-effectiveness of preventive interventions against cervical cancer in Mexican women: results of a Markov model from the public sector perspective]},
	volume = {50},
	issn = {1606-7916},
	shorttitle = {{[Generalized} cost-effectiveness of preventive interventions against cervical cancer in Mexican women},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/18372991},
	abstract = {{OBJECTIVE}

To develop a generalized cost-effectiveness analysis {(GCEA)} of the {HPV} vaccine, hybrid capture screening {(HC)} and Papanicolaou screening {(Pap)} in the Mexican context.


{MATERIALS} {AND} {METHODS}

From April to August 2007, in Mexico, a {GCEA} of the interventions was developed for 10 possible scenarios using a Markov model from the public sector perspective as payer.


{RESULTS}

