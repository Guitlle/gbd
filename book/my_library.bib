	journal = {{PloS} One},
	author = {Dubar, Grégory and Azria, Elie and Tesnière, Antoine and Dupont, Hervé and Le Ray, Camille and Baugnon, Thomas and Matheron, Sophie and Luton, Dominique and Richard, {Jean-Christophe} and Launay, Odile and Tsatsaris, Vassilis and Goffinet, François and Mignon, Alexandre},
