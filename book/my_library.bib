	urldate = {2012-07-11},
	journal = {Alternative therapies in health and medicine},
	author = {Singh, {BB} and Berman, {BM} and Simpson, {RL} and Annechild, A},
	month = may,
	year = {1998},
	note = {{PMID:} 9581324},
	keywords = {Adult, Aged, Complementary Therapies, Female, Humans, Incidence, Middle Aged, Premenstrual Syndrome},
	pages = {75--79}
},

