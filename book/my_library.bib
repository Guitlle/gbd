	keywords = {Adolescent, Adult, Aged, Blood-Borne Pathogens, Child, Cohort Studies, Egypt, Female, Hepatitis C, Humans, Injections, Intravenous, Male, Middle Aged, Risk Factors, Schistosomiasis, Schistosomicides},
