	journal = {Lancet},
	author = {Frank, C and Mohamed, {MK} and Strickland, {GT} and Lavanchy, D and Arthur, {RR} and Magder, {LS} and El Khoby, T and Abdel-Wahab, Y and Aly Ohn, {ES} and Anwar, W and Sallam, I},
