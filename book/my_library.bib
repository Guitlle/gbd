@article{a_applying_2011,
	title = {Applying a system approach to forecast the total hepatitis C virus-infected population size: model validation using {US} data},
	volume = {31 Suppl 2},
	issn = {1478-3231},
	shorttitle = {Applying a system approach to forecast the total hepatitis C virus-infected population size},
	url = {http://www.ncbi.nlm.nih.gov/pubmed/21651700},
	doi = {10.1111/j.1478-3231.2011.02535.x},
	abstract = {{BACKGROUND}

Hepatitis C virus {(HCV)} infection is associated with chronic progressive liver disease. Its global epidemiology is still not well ascertained and its impact will be confronted with a higher burden in the next decade. Aim: The goal of this study was to develop a tool that can be used to predict the future prevalence of the disease in different countries and, more importantly, to understand the cause and effect relationship between the key assumptions and future trends.


{METHODS}

A system approach was used to build a simulation model where each population was modeled with the appropriate inflows and outflows. Sensitivity analysis was used to identify the key drivers of future prevalence.


{RESULTS}

The total {HCV-infected} population in the {US} was estimated to decline 24\% from 3.15 million in 2005 to 2.47 million in 2021, while disease burden will increase as the remaining infected population ages. During the same period, the mortality rate was forecasted to increase from 2.1 to 3.1\%. The diagnosed population was 50\% of the total infections, while less than 2\% of the total infections were treated.


{CONCLUSION}

We have created a framework to evaluate the {HCV-infected} populations in countries around the world. This model may help assess the impact of policies to meet the challenges predicted by the evolution of {HCV} infection and disease. This prediction tool may help to target new public health strategies.},
	urldate = {2012-07-11},
	journal = {Liver international: official journal of the International Association for the Study of the Liver},
	author = {A, Dobson and Razavi, {HA} and Cooper, {CL} and Alberti, A and Dusheiko, {GM} and Pol, S and Zuckerman, E and Koike, K and Han, {KH} and Wallace, {CM} and Zeuzem, S and Negro, F},
	month = jul,
	year = {2011},
	note = {{PMID:} 21651700},
	keywords = {Adult, Aged, Bayes Theorem, Computer Simulation, Epidemics, Europe, Female, Hepatitis C, Chronic, Humans, Incidence, Male, Middle Aged, Numerical Analysis, Computer-Assisted, Prevalence, Reproducibility of Results, Risk Assessment, Risk Factors, Time Factors, Uncertainty, United States},
	pages = {4--17}
},

