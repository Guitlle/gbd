	author = {Mackenbach, Johan P and Bos, Vivian and Andersen, Otto and Cardano, Mario and Costa, Giuseppe and Harding, Seeromanie and Reid, Alison and Hemström, Örjan and Valkonen, Tapani and Kunst, Anton E},
